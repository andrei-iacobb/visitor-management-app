{
  "name": "visitor-management-backend",
  "version": "1.0.0",
  "description": "Backend API for Visitor Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node tests/api-test.js",
    "test:comprehensive": "node tests/comprehensive-api-test.js",
    "test:stress": "node tests/stress-test.js",
    "test:all": "npm run test && npm run test:comprehensive",
    "db:setup": "psql -U postgres -f database/schema.sql",
    "archive:dry-run": "node scripts/archive-old-records.js --dry-run",
    "archive:run": "node scripts/archive-old-records.js"
  },
  "keywords": [
    "visitor",
    "management",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.0.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
